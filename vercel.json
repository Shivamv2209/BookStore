{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "GOOGLE_CLIENT_ID": "@google_client_id",
    "GOOGLE_CLIENT_SECRET": "@google_client_secret",
    "SECRET_KEY": "@secret_key",
    "GENSALT": "@gensalt",
    "GOOGLE_CALLBACK_URL": "@google_callback_url"
  },
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "framework": null
}
