{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "MONGO_URI": "@MONGO_URI",
    "GOOGLE_CLIENT_ID": "@GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET": "@GOOGLE_CLIENT_SECRET",
    "SECRET_KEY": "@SECRET_KEY",
    "GENSALT": "@GENSALT"
  },
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "framework": null
}
